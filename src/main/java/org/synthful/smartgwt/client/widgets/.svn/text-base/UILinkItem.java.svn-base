package org.synthful.smartgwt.client.widgets;

import com.smartgwt.client.widgets.form.fields.LinkItem;

public class UILinkItem extends UIFormItem<LinkItem>{

	public UILinkItem(){
		item= new LinkItem();
	}
	
	public UILinkItem(LinkItem item){
		super.item = item;
	}
	
	public void setLinkTitle(String linkTitle){
		super.item.setLinkTitle(linkTitle);
	}
	
	public String getLinkTitle(){
		return super.item.getLinkTitle();
	}

}
